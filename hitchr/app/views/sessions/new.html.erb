<h1>Log In</h1>

<div class="log-in-form">
<%= form_tag sessions_path do %>

  <div class="field">
    <%= label_tag :email %> <br />
    <%= text_field_tag :email %>
  </div>

  <div class="field">
     <%= label_tag :password %><br />
     <%= password_field_tag :password %>
  </div>

  <div class="field">
    <%= submit_tag "Log In" %>
  </div>
</div>

<% end %>
